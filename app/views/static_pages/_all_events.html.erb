 <div class="tab-pane fade show active" 
 id="all_events" role="tabpanel" aria-labelledby="all_events-tab">
 
  <% if @all_events.nil? || @all_events.count < 1 %>
    <div class="mt-4">
      Pas d'Evenements 
    </div>
  <% else %>
  <%= render partial: 'card_event', locals: {events: @all_events} %>
 <% end %>

 <div id="infinite-scrolling">
    <%= will_paginate @all_events, renderer:  WillPaginate::ActionView::Bootstrap4LinkRenderer %>
</div>
 
 </div>

