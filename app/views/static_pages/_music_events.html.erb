 <div class="tab-pane fade" 
 id="music_events" role="tabpanel" aria-labelledby="music_events-tab">
 
 <% if @music_events.nil? || @music_events.count < 1 %>
    <div class="mt-4">
      No events to show here
    </div>
  <% else %>
  <%= render partial: 'card_event', locals: {events: @music_events} %>
 <% end %>
 
 </div>